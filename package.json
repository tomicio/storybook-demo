    "@storybook/test": "^7.6.13",
    "@storybook/vue3": "^7.6.13",
    "@storybook/vue3-vite": "^7.6.13",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
